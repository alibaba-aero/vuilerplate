{
  "name": "vuilerplate",
  "private": true,
  "version": "0.0.2",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "lint": "eslint . --ext .js,.ts,.vue",
    "stylelint": "stylelint '**/*.{scss,css,vue}'",
    "release:stg": "standard-version -s -a --prerelease beta && git push --follow-tags",
    "release:prod": "standard-version -s -a && git push --follow-tags",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky install",
    "typecheck": "vue-tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --ext .js,.ts,.vue --fix",
      "pnpm vitest related --run"
    ],
    "*.{scss,vue}": "stylelint --fix"
  },
  "dependencies": {
    "@vueuse/core": "8.9.0",
    "@vueuse/head": "0.7.6",
    "@vueuse/schema-org": "0.6.2",
    "pinia": "2.0.14",
    "vue": "3.2.37",
    "vue-router": "4.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.25.2",
    "@pinia/testing": "0.0.12",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/vue": "6.6.0",
    "@typescript-eslint/parser": "5.30.5",
    "@vitejs/plugin-vue": "2.3.3",
    "@vitejs/plugin-vue-jsx": "1.3.10",
    "@vue/test-utils": "2.0.2",
    "c8": "7.11.3",
    "eslint": "8.19.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-vue": "9.2.0",
    "happy-dom": "6.0.2",
    "husky": "8.0.1",
    "jsdom": "20.0.0",
    "lint-staged": "13.0.3",
    "postcss": "8.4.14",
    "postcss-html": "1.4.1",
    "postcss-import": "14.1.0",
    "postcss-nested": "5.0.6",
    "postcss-url": "10.1.3",
    "rollup-plugin-visualizer": "5.6.0",
    "standard-version": "9.5.0",
    "stylelint": "14.9.1",
    "stylelint-config-recommended-vue": "1.4.0",
    "typescript": "4.7.4",
    "unplugin-auto-import": "0.9.2",
    "unplugin-vue-components": "0.21.1",
    "vite": "2.9.13",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pages": "0.25.0",
    "vite-plugin-pwa": "0.12.2",
    "vite-plugin-vue-layouts": "0.6.0",
    "vitest": "0.17.0",
    "vue-eslint-parser": "9.0.3",
    "vue-tsc": "0.38.3"
  }
}
